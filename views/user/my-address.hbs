 <!-- Start Client Shipping Address -->
 <div class="row">
     {{#each myAddress}}
                 <div class="col-md-8 p-5">
                <div class="section-content">
                    <h5 class="section-content__title mt-3">Delivery Details</h5>
                </div>
                <form id="checkout"  class="form-box" >
                    <div class="row"> 
                        <div class="col-md-6">
                            <div class="form-box__single-group">
                                <label for="form-first-name">First Name</label>
                                
                                 <input type="text"  name="user" value="{{this.user}}" hidden>
                                <input type="text" id="first-name" name="form-first-name" value="{{this.firstName}}">
                            </div>
                        </div>
                       
                         <div class="col-md-6">
                            <div class="form-box__single-group">
                                <label for="form-last-name">Last Name</label>
                                <input type="text" id="lastName" name="form-last-name" value="{{this.lastName}}" >
                            </div>
                        </div>
                       
                        <div class="col-md-12">
                            <div class="form-box__single-group">
                                <label for="form-country">Country</label>
                                 <input type="text" id="country" name="form-country" value="{{this.country}}" >
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-box__single-group">
                                <label for="form-address-1">Street Address</label>
                                <input type="text" id="address" name="form-address-1"
                                    placeholder="House number and street name" value="{{this.address}}" >

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-box__single-group">
                                <label for="form-state">State</label>
                                 <input type="text" id="state" name="form-state" value="{{this.state}}">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-box__single-group">
                                <label for="form-zipcode">Postal Code</label>
                                <input type="number" id="zipcode" name="form-zipcode" value="{{this.pincode}}" >
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-box__single-group">
                                <label for="form-phone">Phone</label>
                                <input type="number" id="phone" name="form-phone" value="{{this.mobile}}">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-box__single-group">
                                <label for="form-email">Email Address</label>
                                <input type="email" id="email" name="form-email" value="{{this.email}}">
                                
                            </div>
                        </div>

                    </div>
                <button type="submit"  class="btn btn--block btn--small btn--blue btn--uppercase btn--weight " style="width: 20%;">edit address</button>

                </form>
                {{/each}}
                

              

            </div> <!-- End Client Shipping Address -->


 </div>

            <script src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc=" crossorigin="anonymous"></script>

<script>
     $("#checkout").submit((e) => {
       
        e.preventDefault();
        console.log('im in')
        $.ajax({
            url: '/edit-address',
            method:'post',
             data: $("#checkout").serialize(),
             success:(response)=>{
                 console.log(response)
                 if(response.status){
                     window.location.href='/my-account'
                     
                 }else{
                     alert('something error')
                 }
             }
             })
        })

</script>